.photo-caption
{
    color:#777;
    float:none;
    margin-top: 15px;
}

.photo-modal #caption
{
    max-height: 400px;
}

#photosTab .photo-actions
{
    margin-bottom:15px;
}

#photosTab .photo-actions h2
{
    margin-top:0;
    margin-bottom:0;
}

#photosTab .col-md-8
{
    background: none;
    border: none;

    padding: 0 15px 0 15px;

    box-shadow: none;
}

#blogTab .blog-title
{
    margin-top: 15px;
    margin-bottom: 10px;
}

#blogTab .blog-title h2
{
    margin-top:0;
    margin-bottom: 0;
}

#commentHtml, #postHtml, #newCommentHtml
{
    display: none;
}

#wall-loader
{
    display: none;
    height: 0px;
    opacity:0;
}

#spin-container
{
    height:90px;
    width:90px;
    margin: 0 auto;
    background-color: #ccc;
    border-radius: 4px;
    border:1px solid #c9c9c9;
}


.viewMoreBtn
{
    text-transform: lowercase;
    color:black;
    margin:0px auto 15px auto;
    display: block;
    font-size:1.3em;
    font-weight: 300;
}

.viewMoreBtn:focus, .viewMoreBtn:hover, .viewMoreBtn:active
{
    outline: invert none medium !important;
    text-decoration: none !important;
    border: none !important;
}

.viewMoreBtn i.fa
{
    display: block;
    font-size:2em;
}

.viewMoreBtn:hover
{
    text-decoration: none;
}

.spinner-index
{
    position: relative !important;
    display: block;
}

.tempContent
{
    display: inline-block;
    width:100%;
}

.profile-xs-info
{
    text-align: center;
}

.profile-user .sub-col
{
    padding-left:15px;
    padding-right: 15px;
    margin: 0;
}

#followingTab .row,
#followersTab .row {
    margin-left: 10px;
    padding: 5px;
}
#followingTab button,
#followersTab button {
    margin-left: 5px;
}
/* ---- End of styles for user pages --- */