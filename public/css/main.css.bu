.album-container
{
    width:100%;
    display: table;
}
.album
{
    width:250px;

    min-height:150px;
    background-color: #FFF;

    padding:10px;

    float:left;
    display: table-cell;

    margin-right:15px;
    margin-bottom:15px;
}

.album-preview
{
    background-color: #E1E1E1;
    height:150px;
}

.album-preview:hover
{
    cursor: pointer;
    background-color: #e0deff;
}

.album-preview .fa
{
    height:100%;
    width:100%;
    font-size:7em;
    color:#c1c1c1;
    text-align: center;
    margin-top:30px;
}

.album-preview .fa:hover
{
    color:#c9c6ff;
}

.album-name
{
    color:#0D54D7;
    font-weight:bold;
    text-transform: uppercase;
}

.album-row
{
    margin-bottom:15px;
    margin-left:0;
    margin-right:0;
}

.feed-tabs
{
    border-bottom: none;	
}

.feed-tabs > li.active > a, .feed-tabs > li.active > a:hover, .feed-tabss > li.active > a:focus
{
    background-color: #2B3849 !important;
    border:none;
    color:#FFF !important;
    text-transform: uppercase;
    font-weight: 400;
}

.feed-tabs > li
{
    border-left: none !important;
    border-top:none;
    border-bottom: none;

    border-right:1px solid white;
}

.feed-tabs > li:last-child
{
    border-right:none;
}

.feed-tabs > li > a:hover
{
    background-color: #556f91;
}

.feed-tabs > li > a:hover, .feed-tabs > li > a:focus
{
    background-color: #556f91;
}

.feed-tabs > li > a
{
    display: inline;
    background-color: none !important;

    border:none;


    border-radius: 0;
    color:#FFF;
    text-transform: uppercase;
    font-weight: 300;
    padding:10px 15px 9px 15px;
    margin-right: 0;
}

.postactions-container .tab-content
{
    margin-top:0px;
}

.profile-blog-panel .fileUploadContainer
{
    border:2px solid #c9c6ff;
    width:100%;
    height:80px;
    text-align: center;
    padding-top:20px;
}

.profile-blog-panel .highlight
{
    background:#fcf8cb;
}

.fileProgressBar .progress
{
    margin:10px;
    border-radius: 0;
}

.fileNameTxt
{
    margin:10px 0 0 10px;
}

#photo-post-container
{
    min-height: 150px;
}

#photo-post-container .col-md-8
{
    padding-bottom: inherit;
    padding-top:inherit;
    border: none;
    box-shadow: inherit;
    background: none;
}

#photo-post-container textarea
{
    height:150px;
    border:none;
    resize: none;
    box-shadow: none;
}

#imgPreviewContainer
{
    margin-left:10px;
    margin-bottom: 10px;
    margin-top: 10px;
}

.photo-modal
{
    overflow-y: hidden;
}

.photo-modal .modal-dialog
{
    width:auto;
    padding-left:30px;
    padding-right:30px;
}

.photo-modal .modal-content
{
    background-color:#e9e9ec;
    min-height: 400px;
}


.photo-modal .modal-body
{
    padding:0px;
}

.photo-modal .modal-body .img-bg
{
    background-color: black;
    padding:0;
    text-align: center;
    margin-bottom:0 !important;
}

.photo-modal #photo-img
{
    margin:0 auto;
}

.photo-modal .modal-body .photo-content
{
    margin-left:0;
    margin-right:0;
}

.photo-modal .modal-header
{
    padding:0 15px 0 0;
    min-height: inherit;
}

#dyn-ajax-loader
{
    margin:0 auto;
    display: block;
}

#stream-display a
{
    outline:none;
}

#stream-display a canvas
{
    position: relative;
    top:50%;
    margin-top:-40px;
}

#photo-info-display
{
    padding-top:15px;
    padding-bottom:15px;
}

.photo-modal .close
{
    display: inline;
    position: absolute;
    right: 15px;
    z-index:999;
}

.photo-modal .slimScrollDiv
{
    margin-left:-15px;
    margin-right:-15px;
}

.photo-modal .wallpost-actions
{
    margin:15px 15px 10px 15px;

}
